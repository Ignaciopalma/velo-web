<%= form_for Biker.new  do |f| %>
	<div class="col-sm-8">
		<%= f.label :nombre %>
		<%= f.text_field :name, class: "form-control" %>
	</div>

	<div class="col-sm-8">
		<%= f.label :email %>
		<%= f.text_field :email, class: "form-control" %>
	</div>

	<div class="col-sm-8">
		<%= f.label :rut %>
		<%= f.number_field :rut, class: "form-control" %>
	</div>

	<div class="col-sm-8">
		<%= f.label :telefono %>
		<%= f.number_field :number, class: "form-control" %>
	</div>

	<div class="col-sm-8">
		<%= f.label :apellido %>
		<%= f.text_field :last_name, class: "form-control" %>
	</div>

	<div class="col-sm-8">
		<%= f.label :direccion %>
		<%= f.text_field :address, class: "form-control" %>
	</div>
	
	<div class="col-sm-8">
		<%= f.label :Banco %>
		<%= f.text_field :bank, class: "form-control" %>
	</div>
	
	<div class="col-sm-8">
		<%= f.label :tipo_de_cuenta %>
		<%= f.text_field :acc_type, class: "form-control" %>
	</div>
	
	<div class="col-sm-8">
		<%= f.label :numero_de_cuenta %>
		<%= f.number_field :acc_number, class: "form-control" %>
	</div>

<br>

	<div class="col-sm-8">
		<%= f.submit "Enviar", class: "btn btn-success" %>
	</div>
<% end %>